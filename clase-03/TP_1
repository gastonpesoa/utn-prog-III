1.
SELECT * FROM `Productos` ORDER BY pNombre
2.
SELECT * FROM `Provedores` WHERE Localidad='Quilmes'
3.
SELECT * FROM `Envios` WHERE Cantidad BETWEEN 200 AND 300
4.
SELECT SUM(Cantidad) FROM `Envios`
5.
SELECT pNumero FROM `Envios` LIMIT 3
6.
SELECT a.Nombre, b.pNombre FROM Envios AS e, Provedores AS a, Productos AS b WHERE e.Numero = a.Numero AND e.pNumero = b.pNumero
7.
SELECT e.Cantidad * b.Precio FROM Envios AS e, Productos AS b WHERE e.pNumero = b.pNumero
8.
SELECT SUM(Cantidad) FROM Envios WHERE Numero = 102 AND pNumero = 1
9.
SELECT b.pNumero FROM Envios AS e, Provedores AS a, Productos AS b WHERE a.Localidad = 'Avellaneda' AND e.Numero = a.Numero AND e.pNumero = b.pNumero
10.
SELECT Domicilio, Localidad FROM Provedores WHERE Nombre LIKE '%I%'
